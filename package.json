{
  "name": "fsolauncher",
  "version": "1.4.8",
  "description": "Install, play, and configure FreeSO easily",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . fsolauncher --asar --platform=win32 --arch=all --overwrite --electron-version=3.0.3 --version-string.FileDescription='FreeSO Launcher' --out ../../release",
    "builddarwin": "electron-packager . fsolauncher --asar --platform=darwin --arch=all --overwrite --electron-version=3.0.3 --version-string.FileDescription='FreeSO Launcher' --out ../../release",
    "buildasar": "asar pack . \"../../release/asar/app.asar\" --unpack-dir \"{export,FSOLauncher_UI/FSOLauncher_Images,FSOLauncher_UI/FSOLauncher_Sounds,FSOLauncher_UI/FSOLauncher_Fonts}\""
  },
  "keywords": [
    "freeso",
    "launcher"
  ],
  "author": "FreeSO.org",
  "devDependencies": {
    "electron-reload": "^1.2.5"
  },
  "dependencies": {
    "cloudscraper": "^1.4.1",
    "csv": "^1.1.1",
    "electron-pug": "^1.5.1",
    "fs-extra": "^3.0.1",
    "ini": "^1.3.4",
    "md5-file": "^3.1.1",
    "mkdirp": "^0.5.1",
    "os-locale": "^2.0.0",
    "request": "^2.88.0",
    "rss-parser": "^2.7.0",
    "node-unzip-2": "^0.2.7",
    "windows-shortcuts": "^0.1.5",
    "winreg": "^1.2.4"
  }
}
